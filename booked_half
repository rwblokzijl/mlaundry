    });

	$("div.DivCalendarObjectTijdsBlokNotBooked").mouseout(function(){
		$( this ).css( "border-color", "black" );
		$( this ).css( "background-color", "white" );
    });

});


function GetBrowserDate()
  {
    var fullDate = new Date();console.log(fullDate);
    var twoDigitMonth = (fullDate.getMonth()+1)+"";if(twoDigitMonth.length==1)	twoDigitMonth="0" +twoDigitMonth;
    var twoDigitDate = fullDate.getDate()+"";if(twoDigitDate.length==1)	twoDigitDate="0" +twoDigitDate;
    var BrowserDate = twoDigitDate + "-" + twoDigitMonth + "-" + fullDate.getFullYear();
	return BrowserDate;
  }

function UpdateScreen()
    {
		//alert(sDateSelect+'!'+$('#date').val());
		//alert( '1' + $('#date').val() + '2');
		//var CalDiv = document.getElementById('CalDiv');
		//if 	( $(CalDiv).is(":visible")  )
		//if ( $('#CalDiv').is(':hidden') )
		//	{
		//		alert('ja');
		//	}

		if ( $('#CalDiv').css('visibility') == 'visible' )
			{
				if ( $('#SelectBlocks').css('display') == 'none' )
					{

					}	else {
						$('#SelectBlocks').css( 'display', 'none' );
					}

			} else {

				if ( $('#SelectBlocks').css('display') == 'none' )
					{
						if ( sDateSelect != $('#date').val() && $('#date').val() != null ) //  andere datum geselecteerd dus vernieuwen
							{

								//clearInterval(intervalUpdateScreen); // stop the interval
								LoadNewDate();
							} else { // geen andere datum geselecteerd dus weer presenteren
								$('#SelectBlocks').css( 'display', 'block' );
							}

					}	else {


					}

			}



			if ( sDateSelect != $('#date').val() && $('#date').val() != null ) //  andere datum geselecteerd dus vernieuwen
				{
					LoadNewDate();
				}



	}

 function LoadNewDate()
	{
		sDateSelect = $('#date').val();
		var NewUrl = "findavailableFromMachineType.php?Start_Date=" + sDateSelect;
		$('#CalendarObject').css( 'display', 'none' );
		$( "#content1" ).load( NewUrl , function() {
		});
	}


