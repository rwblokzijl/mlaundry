 --><html><head><script language="javascript" type="text/javascript">
/*JAVASCRIPT FUNCTIONS*/
function CheckValue(varX)
  {
  var index = document.getElementById(varX).selectedIndex;
  var value = document.getElementById(varX).options[index].text;

  if(value.indexOf(':') >= 0)
    {
    //Geselecteerde tijd splitsen
    var HMS = value.split(':')
        HMS[0] = parseInt(HMS[0],10) * 60;
        HMS[1] = parseInt(HMS[1],10) + 90;

    //210 minuten (3,5uur) bij optellen
    var TotMins = HMS[0] + HMS[1];

    //Eind uur berekenen
    var H = parseInt(TotMins, 10) / 60;
        H = '' + (Math.floor(H));

    //Eventueel een 0 toevoegen aan uur
    var Hlen = H.length
    H = (Hlen == 1) ? "0" + H : H;

    //Eind minuut berekenen
    var M = parseInt(TotMins, 10) % 60;
    //indien nodig een 0 ervoor zetten
    M = (M < 10 ? '0' : '') + M;

    //Tijd opbouwen en presenteren
    document.getElementById('Lbl-'+varX).innerHTML = ' t/m ' + H + ':' + M;
    }
  else
    {
    //Er is geen tijd geselecteerd, dus niks weergeven
    document.getElementById('Lbl-'+varX).innerHTML = '';
    }
  }

function AddNew(id,from,untill,name,title,addnew)
  {
  var ElemExist = document.getElementById("td-"+name+"-"+title);
  //Bestaat het element al?
  if(ElemExist == null)
    {
    //Element aanmaken waarin het pulldown menu komt
    $("#table-"+name).append("<tr>\n<td align=\"center\" width=\"97\"></td><td align=\"center\" width=\"97\" id=\"td-"+name+"-"+title+"\"></td>\n<td width=\"97\" align=\"center\">\n<label id=\"Lbl-"+name+"-"+title+"\"></label></td>\n</tr>\n");
    }

  //Pulldown menu renderen en in het element plaatsen
  $("#td-"+name+"-"+title).load('pulldown.php?start='+from+'&end='+untill+'&name='+name+'&title='+title+'&AddNew='+addnew);
  //Bijbehorende label leeg maken
  document.getElementById('Lbl-'+name+'-'+title).innerHTML = "";
  }

function GetStartTime(var1,var2)
  {
  var StartTime = document.getElementById('Lbl-'+var1+'-'+var2).innerHTML;

  StartTime = StartTime.replace("t/m","");
  StartTime = StartTime.replace(/ /g,"");

  return StartTime;
  }

function ClearBlok(name, title)
  {
  document.getElementById('td-'+name+'-'+title).innerHTML = "";
  }

function DisableCheck(TheBool, id)
  {

  var TheElement = document.getElementById(id);

  if(TheBool == true)
    {
    TheElement.checked = false;
    TheElement.disabled = true;
    }
  else
    {
    TheElement.checked = true;
    TheElement.disabled = false;
    }
  }
/*END JAVASCRIPT FUNCTIONS*/
</script><!--
